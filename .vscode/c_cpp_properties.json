{
  "configurations": [
    {
      "name": "Linux",
      "includePath": [
        "${workspaceFolder}/**",
        "/usr/include/**"
      ],
      "compilerPath": "/usr/bin/clang",
      "cStandard": "c17",
      "cppStandard": "gnu++17",
      "intelliSenseMode": "linux-clang-x64",
      "compilerArgs": [
        "-Wall",
        "-Wextra",
        "-Werror"
      ],
      "configurationProvider": "ms-vscode.cmake-tools"
    }
  ],
  "version": 4
}